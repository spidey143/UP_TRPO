<Window x:Class="Legkaya.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Legkaya"
        mc:Ignorable="d"
        Title="MainWindow" Height="460" Width="800">
    <Grid>
        <Button x:Name="STAFF" Content="Отдел Кадров" Margin="10,10,645,384" RenderTransformOrigin="0.966,0.557" Click="STAFF_Click" FontSize="14" Height="NaN" Width="NaN" VerticalContentAlignment="Top"/>
        <Button x:Name="PLANS" Content="Отдел Планирования" Margin="400,10,229,384" RenderTransformOrigin="0.966,0.557" FontSize="14" Height="NaN" Width="NaN" VerticalContentAlignment="Top" Click="PLANS_Click"/>
        <Button x:Name="ECO" Content="ЭКО" Margin="183,10,421,384" RenderTransformOrigin="0.966,0.557" FontSize="14" Click="ECO_Click" VerticalContentAlignment="Top"/>
        <Button x:Name="ZAVOD" Content="Отдел производства" Margin="595,10,10,384" RenderTransformOrigin="0.966,0.557" FontSize="14" Height="NaN" Width="NaN" VerticalContentAlignment="Top" Click="ZAVOD_Click"/>
        <Frame x:Name="FRAME" Background ="Chocolate" Content="" Height="NaN" Margin="10,65,10,10" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
